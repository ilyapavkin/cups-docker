# default: off
# description: The sane server accepts requests
# for network access to a local scanner via the
# network.
service sane-port
{
   port        = 6566
   socket_type = stream
   wait        = no
   user        = saned
   group       = lp
   server      = /usr/sbin/saned
   log_type    = SYSLOG daemon info
}
